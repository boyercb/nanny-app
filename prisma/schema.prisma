generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("PRISMA_DATABASE_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL") // uses a direct connection
}

model Shift {
  id         Int      @id @default(autoincrement())
  start      DateTime
  end        DateTime
  hourlyRate Float
  title      String?
}
